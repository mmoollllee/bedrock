{
   "name": "wordpress-template",
   "version": "1.0.0",
   "description": "Build Tools for Wordpress Template",
   "author": "Moritz Graf",
   "private": false,
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "git://github.com/mmoollllee/trellis-template.git"
   },
   "scripts": {
      "build": "mix",
      "build:production": "mix --production",
      "start": "mix watch",
      "hot": "mix watch --hot",
      "test": "npm run lint",
      "lint": "npm run lint:js && npm run lint:css",
      "lint:js": "eslint resources/scripts",
      "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
      "icons": "fantasticon -c src/icons/.fantasticonrc.js",
   },
   "devDependencies": {
      "@wordpress/babel-preset-default": "^6.13.0",
      "@wordpress/browserslist-config": "^4.1.2",
      "@wordpress/dependency-extraction-webpack-plugin": "^3.6.0",
      "babel-eslint": "^10.1.0",
      "browser-sync": "^2.27.10",
      "browser-sync-webpack-plugin": "^2.3.0",
      "eslint": "^8.18.0",
      "eslint-plugin-import": "^2.26.0",
      "fantasticon": "*",
      "laravel-mix": "*",
      "postcss": "^8.4.14",
      "sass": "^1.52.3",
      "sass-loader": "13.0.0",
      "stylelint": "^14.9.1",
      "stylelint-config-standard": "^26.0.0"
   },
   "dependencies": {
      "@fancyapps/ui": "*",
      "bootstrap": "*",
      "entypo": "*",
      "jquery": "*",
      "jquery-on-screen": "*"
   }
}
