{
  "name": "mmoollllee/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, based on roots/bedrock. Shipped with a custom Wordpress template and some kind of page-builder",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "composer", "roots", "wordpress", "wp", "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
    },
    {
      "type":"composer",
      "url":"https://connect.advancedcustomfields.com"
    },
    {
        "type": "vcs",
        "url": "https://github.com/mmoollllee/bedrock-deployer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.2",
    "vlucas/phpdotenv": "^5.5",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": "6.3.1",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.1.0",
    "wpengine/advanced-custom-fields-pro": "*",
    "stoutlogic/acf-builder": "1.12.0",
    "wpackagist-plugin/autoptimize": "^3.0",
    "wpackagist-plugin/contact-form-7": "^5.4",
    "wpackagist-plugin/contact-form-7-honeypot": "^2.1",
    "wpackagist-plugin/duplicate-post": "^4.1",
    "wpackagist-plugin/google-sitemap-generator": "^4.1",
    "wpackagist-plugin/redirection": "^5.1",
    "wpackagist-plugin/resmushit-image-optimizer": "^0.4.1",
    "wpackagist-plugin/wp-statistics": "^13.1",
    "wpackagist-plugin/wp-super-cache": "^1.7",
    "wpackagist-plugin/save-with-keyboard": "^3.0.2",
    "wpackagist-plugin/simple-history":"^3.3",
    "wpackagist-plugin/duplicate-menu":"^0.2",
    "wpackagist-plugin/rename-wp-login":"^2.6",
    "wpackagist-plugin/regenerate-thumbnails": "^3.1",
    "wpackagist-plugin/wp-nested-pages": "^3.1",
    "mmoollllee/bedrock-deployer": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.1",
    "roave/security-advisories": "dev-latest",
    "wpackagist-plugin/wordpress-importer": "^0.8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true,
      "pivvenit/acf-pro-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin", "wpengine/advanced-custom-fields-pro", "wpackagist-plugin/duplicate-post", "wpackagist-plugin/save-with-keyboard", "wpackagist-plugin/simple-taxonomy-ordering",  "wpackagist-plugin/duplicate-menu"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
